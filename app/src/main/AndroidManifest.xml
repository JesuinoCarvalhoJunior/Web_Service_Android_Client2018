<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wsandroid.jcarvalhojr.webserviceandroidcliente"
    android:versionCode="1"
    android:versionName="Web Service" >


    <uses-sdk android:minSdkVersion="14" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.Provider.ApplicationContextProvider"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.CadastroActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.BuscaPorIdActivity"
            android:label="@string/title_activity_busca_por_id" >
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.AtualizarDadosActivity"
            android:label="@string/title_activity_atualizar_dados" >
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.Detalhes_Activity"
            android:label="@string/title_activity_detalhes_" >
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.MainActivityLogin"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.wsandroid.jcarvalhojr.webserviceandroidclient.RestritoActivity"
            android:label="@string/title_activity_restrito"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>